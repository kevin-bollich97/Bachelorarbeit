{
    "version": "0.1",
    "enabled": true,
    "language": "en,de",
    "dictionaryDefinitions": [
        {
            "name": "projectDictionary",
            "path": "./dictionary.txt"
        }
    ],
    "dictionaries": [
        "projectDictionary"
    ],
    "languageSettings": [
