        "\\\\end{\\w+}",
        "\\\\usepackage{\\w+}",
        "\\\\bibliographystyle{\\w+}",
        "\\\\hyphenation{[A-Za-z0-9, -]+}"
    ],
    "words": [
        "Graylog",
        "Kibana",
        "Logstash"
    ]
}